<%= form_for (@project) do |f| %>

<div class="field">
  <%= f.label :title %> <br/>
  <%= f.text_field :title %>
</div>

<div class="field">
  <%= f.label :description %> <br/>
  <%= f.text_area :description %>
</div>
<div class="field">
  <%= f.label :funding_goal %> <br/>
  <%= f.number_field :funding_goal %>
</div>
<div class="field">
  <%= f.label :start_date %> <br/>
  <%= f.text_field :start_date %>
</div>
<div class="field">
  <%= f.label :end_date %> <br/>
  <%= f.text_field :end_date %>
</div>

<div class="action">
  <%= f.submit %>
</div>
  <% end %>
